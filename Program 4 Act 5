#include<iostream>
using namespace std;

int main()
{
    float num, sum=0, average;
    for (int i=1; i<=4; i++)
    {
        do
        {
            cout<<"enter quarter"<<i<<": ";
            cin>>num;
        }
        while(num<60 || num>100);
        
        sum=sum+num;
    }
    
    average=sum/4;
    cout<<"the average is"<<average<<endl;
}